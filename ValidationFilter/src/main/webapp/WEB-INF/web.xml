  </servlet>
  <servlet-mapping>
    <servlet-name>StudentRegServlet</servlet-name>
    <url-pattern>/stdreg</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>ValidationFilter</display-name>
    <filter-name>ValidationFilter</filter-name>
    <filter-class>controller.ValidationFilter</filter-class>
	 	<init-param>
	 		<param-name>key</param-name>
	 		<param-value>MyFilter</param-value>
	 	</init-param>
  </filter>
  <filter-mapping>
    <filter-name>ValidationFilter</filter-name>
    <url-pattern>/stdreg</url-pattern>
  </filter-mapping>
</web-app>